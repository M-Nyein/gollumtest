<%= form_for @adminuser, url: {action: "update"} do |f| %>

	<div>
	<%= f.label :name %>
	<%= f.text_field :name %>
	</div>
	
	<div>
	<%= f.label :email %>
	<%= f.email_field :email %>
	</div>

	<div>
		<%= f.label :password %>
		<%= f.password_field :password%>
	</div>
	<div class="action"><%= f.submit "Update"  %></div>

<% end %>