<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Town</th>
      <th>Division</th>
      <th>Photo</th>
    </tr>
</thead>
  <tbody>
    
      <tr>
        <td><%= @user.name %></td>
        <td><%= @user.email %></td>
        <td><%= @user.phone %></td>
        <td><%= @user.town_id %></td>
        <td><%= @user.division_id %></td>
        <td> </td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),edit_user_path(@user), :class => 'btn btn-primary btn-xs' %>
          
        </td>
      </tr>
  </tbody>
</table>